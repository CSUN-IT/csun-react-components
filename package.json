{
  "name": "csun-components",
  "version": "0.2.0",
  "private": false,
  "main": "src/index.js",
  "dependencies": {
    "marked": "^0.3.16",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-15": "^1.1.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "jest": "22.4.0",
    "jest-cli": "^22.4.2",
    "node-sass": "^4.9.3",
    "react-addons-test-utils": "^15.6.2",
    "react-scripts": "0.9.5",
    "react-test-renderer": "^15.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.11.1"
  },
  "scripts": {
    "start": "yarn run dev-server",
    "test": "yarn jest",
    "watch": "yarn jest --watch",
    "build-dist-js": "webpack --config webpack/dist.conf.js --progress --colors",
    "build-dist-js-min": "webpack --config webpack/dist-min.conf.js --progress --colors",
    "dev-server": "webpack-dev-server --config webpack/dev.conf.js --progress --colors --hot --inline",
    "build-lib": "rm -rf lib && webpack --config webpack/npm.conf.js --progress --colors"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  }
}
